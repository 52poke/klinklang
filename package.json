{
  "name": "klinklang",
  "description": "A collection of utilities of 52Pok√© Wiki.",
  "version": "0.4.0",
  "author": {
    "name": "mudkipme",
    "url": "https://mudkip.me",
    "email": "i@mudkip.me"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Snowpack,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run start-client\" \"npm run watch-server\" \"npm run serve-debug\"",
    "start-client": "snowpack dev",
    "build": "npm run build-client && npm run build-server",
    "build-client": "snowpack build",
    "build-server": "tsc -p ./tsconfig-server.json",
    "serve": "node dist/index.js",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-server": "tsc -w -p ./tsconfig-server.json",
    "format": "eslint . --fix",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudkipme/klinklang.git"
  },
  "dependencies": {
    "@koa/router": "^9.3.1",
    "@material/mwc-icon-button": "^0.17.2",
    "@material/mwc-top-app-bar": "^0.17.2",
    "@types/cheerio": "^0.22.21",
    "@types/convict": "^5.2.1",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^4.0.0",
    "@types/koa-json-error": "^3.1.2",
    "@types/koa-redis": "^4.0.0",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/validator": "^13.1.0",
    "cheerio": "^1.0.0-rc.3",
    "convict": "^6.0.0",
    "haunted": "^4.7.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-json-error": "^3.1.2",
    "koa-redis": "^4.0.1",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "lit-html": "^1.2.1",
    "node-fetch": "^3.0.0-beta.7",
    "oauth-1.0a": "^2.2.6",
    "pg": "^8.3.0",
    "sequelize": "^6.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "snowpack": "^2.6.4",
    "typescript": "^3.9.7"
  }
}
