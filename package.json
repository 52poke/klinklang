{
  "name": "klinklang",
  "description": "A collection of utilities of 52Pok√© Wiki.",
  "version": "0.2.0",
  "author": {
    "name": "mudkipme",
    "url": "https://mudkip.me",
    "email": "i@mudkip.me"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./public/build",
    "build-server": "webpack --config webpack.server.js",
    "build-client": "NODE_ENV=production webpack",
    "build-dev": "NODE_ENV=development webpack",
    "build": "npm run build-client && npm run build-server",
    "lint": "eslint .",
    "start": "NODE_ENV=production node ./build/klinklang",
    "start-dev": "NODE_ENV=development babel-node ./klinklang",
    "radar": "node ./build/radar",
    "radar-dev": "NODE_ENV=development babel-node ./radar"
  },
  "bin": {
    "klinklang": "./build/klinklang.js",
    "radar": "./build/radar.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudkipme/klinklang.git"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.13.0",
    "csv-parse": "^2.0.0",
    "csv-stringify": "^2.0.1",
    "jss": "^9.5.1",
    "jss-preset-default": "^4.0.1",
    "koa": "2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-json-error": "^3.1.2",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "koa-x-hub": "^1.0.0",
    "kue": "^0.11.6",
