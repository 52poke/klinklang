{
  "name": "klinklang",
  "description": "A collection of utilities of 52Pok√© Wiki.",
  "version": "0.2.2",
  "author": {
    "name": "mudkipme",
    "url": "https://mudkip.me",
    "email": "i@mudkip.me"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./public/build",
    "build-server": "webpack --config webpack.server.js",
    "build-client": "NODE_ENV=production webpack",
    "build-dev": "NODE_ENV=development webpack",
    "build": "npm run build-client && npm run build-server",
    "lint": "eslint .",
    "start": "NODE_ENV=production node ./build/klinklang",
    "start-dev": "NODE_ENV=development babel-node ./klinklang",
    "radar": "node ./build/radar",
    "radar-dev": "NODE_ENV=development babel-node ./radar"
  },
  "bin": {
    "klinklang": "./build/klinklang.js",
    "radar": "./build/radar.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudkipme/klinklang.git"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "bull": "^3.5.2",
    "commander": "^2.19.0",
    "csv-parse": "^4.3.0",
    "csv-stringify": "^5.1.2",
    "ipaddr.js": "^1.8.1",
    "json-predicate": "^0.9.5",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-favicon": "^2.0.1",
    "koa-json-error": "^3.1.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "mz": "^2.7.0",
    "nconf": "^0.10.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "tough-cookie-file-store": "^1.2.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.11.1",
    "eslint-plugin-react": "^7.12.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
